      // EXCHANGE
      .exchange
        .container
          .row
            .col-sm-10.offset-sm-1.col-md-8.offset-md-2.col-sm-8.col-lg-6.offset-lg-0
              .counter__block.counter__block--exchange
                .counter__block__exchange
                  .counter__block__heading Отдаёте  →
                  .counter__block__filters
                    .counter__block__filter(data-filter='')
                      span Все
                    .counter__block__filter.active(data-filter='RUB')
                      span ₽
                      |  RUB
                    .counter__block__filter(data-filter='USD')
                      span $
                      |  USD
                    .counter__block__filter(data-filter='BTC')
                      span ฿
                      |  BTC
                    .counter__block__filter(data-filter='DASH')
                      span D
                      |  DASH
                    .counter__block__filter(data-filter='ETH')
                      span E
                      |  ETH
                    .counter__block__filter(data-filter='LTC')
                      span Ł
                      |  LTC
                    .counter__block__filter(data-filter='UAH')
                      span ₴
                      |  UAH
                      
                .exchange-list
                  .exchange-list__item(data-currency="btc").active
                    .exchange-list__item__inner
                      i.ico.ico-currency.ico-currency--bitcoin-yellow
                      .exchange-list__item__name Bitcoin
                  .exchange-list__item(data-currency="btc")
                    .exchange-list__item__inner
                      i.ico.ico-currency.ico-currency--litecoin
                      .exchange-list__item__name Litecoin
                  .exchange-list__item(data-currency="btc")
                    .exchange-list__item__inner
                      i.ico.ico-currency.ico-currency--ethereum
                      .exchange-list__item__name Ethereum

            .col-sm-10.offset-sm-1.col-md-8.offset-md-2.col-sm-8.col-lg-6.offset-lg-0
              .counter__block.counter__block--exchange
                .counter__block__exchange
                  .counter__block__heading Отдаёте  →
                  .counter__block__filters.counter__block__filters--second
                    .counter__block__filter(data-filter="btc") <span>฿</span> BTC
                    .counter__block__filter.active(data-filter="rur") <span>₽</span> RUR
                    .counter__block__filter(data-filter="usd") <span>$</span> USD
                    .counter__block__filter(data-filter="eur") <span>€</span> EUR

                .exchange-list
